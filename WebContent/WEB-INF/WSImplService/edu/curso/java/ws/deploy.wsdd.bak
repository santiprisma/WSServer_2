<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WSImplService WSDL service -->

  <service name="WSImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.java.curso.edu"/>
      <parameter name="wsdlServiceElement" value="WSImplService"/>
      <parameter name="schemaQualified" value="http://ws.java.curso.edu"/>
      <parameter name="wsdlServicePort" value="WSImpl"/>
      <parameter name="className" value="edu.curso.java.ws.WSImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WSImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="recuperarPersonas" qname="operNS:recuperarPersonas" xmlns:operNS="http://ws.java.curso.edu" returnQName="retNS:recuperarPersonasReturn" xmlns:retNS="http://ws.java.curso.edu" returnType="rtns:Persona" xmlns:rtns="http://ws.java.curso.edu" soapAction="" >
      </operation>
      <operation name="recuperarPersonaPorId" qname="operNS:recuperarPersonaPorId" xmlns:operNS="http://ws.java.curso.edu" returnQName="retNS:recuperarPersonaPorIdReturn" xmlns:retNS="http://ws.java.curso.edu" returnType="rtns:Persona" xmlns:rtns="http://ws.java.curso.edu" soapAction="" >
        <parameter qname="pns:idPersona" xmlns:pns="http://ws.java.curso.edu" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="recuperarPersonaPorId recuperarPersonas"/>

      <typeMapping
        xmlns:ns="http://ws.java.curso.edu"
        qname="ns:Persona"
        type="java:edu.curso.java.ws.Persona"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
